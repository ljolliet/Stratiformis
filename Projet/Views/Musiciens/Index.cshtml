@model IEnumerable<Projet.Musicien>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nom_Musicien)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Prenom_Musicien)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Annee_Naissance)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Annee_Mort)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Photo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Genre.Libelle_Abrege)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Instrument.Nom_Instrument)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Pays.Nom_Pays)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nom_Musicien)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Prenom_Musicien)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Annee_Naissance)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Annee_Mort)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Photo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Genre.Libelle_Abrege)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Instrument.Nom_Instrument)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Pays.Nom_Pays)
        </td>
        <td>
            @Html.ActionLink("Details", "Details", new { id=item.Code_Musicien }) 

        </td>
    </tr>
}

</table>
