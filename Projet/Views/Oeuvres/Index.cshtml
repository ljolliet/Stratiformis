@model IEnumerable<Projet.Oeuvre>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Titre_Oeuvre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Sous_Titre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Tonalite)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Annee)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Opus)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Numéro_Opus)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Type_Morceaux.Libelle_Type)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Titre_Oeuvre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Sous_Titre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tonalite)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Annee)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Opus)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Numéro_Opus)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Type_Morceaux.Libelle_Type)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.Code_Oeuvre }) |
                @Html.ActionLink("Details", "Details", new { id=item.Code_Oeuvre }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.Code_Oeuvre })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
